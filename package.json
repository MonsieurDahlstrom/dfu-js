{
  "name": "dfu-beamer",
  "version": "0.1.3",
  "description": "a web bluetooth library for performing DFU of Nordic Semiconductor SOCs. Includes Vue.js Mixin for simplifed use.",
  "main": "dist/index.js",
  "private": false,
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.1",
    "chance": "^1.0.10",
    "eslint": "^5.16.0",
    "factory-girl": "^5.0.4",
    "mocha": "^6.1.4",
    "sinon": "^7.3.2"
  },
  "scripts": {
    "lint": "eslint src tests/unit",
    "build": "babel src --out-dir dist",
    "prepublish": "yarn run build",
    "test": "yarn run lint && yarn run test:unit",
    "test:unit": "mocha --require babel-core/register --require babel-polyfill tests/unit/**/*spec.js --reporter spec"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MonsieurDahlstrom/web-bluetooth-dfu.git"
  },
  "keywords": [
    "web",
    "bluetooth",
    "DFU",
    "nRF5",
    "Nordic",
    "Semiconductor",
    "nrf51",
    "nrf52"
  ],
  "author": "Mathias Dahlstrom",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MonsieurDahlstrom/web-bluetooth-dfu/issues"
  },
  "homepage": "https://github.com/MonsieurDahlstrom/web-bluetooth-dfu#readme",
  "dependencies": {
    "async": "^3.0.1",
    "crc": "^3.4.4",
    "jszip": "^3.1.4"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "sourceMaps": "inline"
  }
}
